<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.github.Franfuu.view.PeluqueroServicio">
    <children>
        <TextField fx:id="nombreField" layoutX="200.0" layoutY="200.0" prefHeight="40.0" prefWidth="400.0" promptText="Nombre del Servicio" />
        <TextField fx:id="precioField" layoutX="200.0" layoutY="260.0" prefHeight="40.0" prefWidth="400.0" promptText="Precio del Servicio" />
        <Button layoutX="200.0" layoutY="320.0" mnemonicParsing="false" onAction="#onGuardarServicio" text="Guardar Servicio" />
        <Button layoutX="200.0" layoutY="380.0" mnemonicParsing="false" onAction="#onVolver" text="Volver" />
    </children>
</AnchorPane>